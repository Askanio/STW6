@using ScrewTurn.Wiki.Web.Models
@model IEnumerable<LanguageViewModel>
@{
    Layout = "~/Views/Install/Layout.cshtml";
}

    <div class="col-md-4 col-md-offset-4">
        <p>Choose a language:</p>
        <ul id="language">
            @foreach (LanguageViewModel languageModel in Model)
            {
                <li>@Html.ActionLink(languageModel.Name, "Step1", new { language = languageModel.Code })</li>}
        </ul>
    </div>

    @section BottomButtons
{

    }
